RewriteEngine On

# # Force HTTPS and www
# RewriteCond %{HTTP_HOST} ^guruscaterers\.com [NC]
# RewriteRule ^(.*)$ https://www.guruscaterers.com/$1 [L,R=301]

# React SPA fallback routing
<IfModule mod_rewrite.c>
  RewriteBase /
  RewriteRule ^index\.html$ - [L]
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteRule . /index.html [L]
</IfModule>
